{
    "name": "pptr-visual",
    "version": "1.0.0",
    "description": "Visual Regression Testing with Puppeteer",
    "main": "index.js",
    "scripts": {
        "test-snapshots": "jest --config=jest.config.js --detectOpenHandles --forceExit",
        "test-snapshots-update": "jest --config=jest.config.js --updateSnapshot --detectOpenHandles --forceExit",
        "test:percy": "percy exec -- jest ./tests-snapshots/__tests__/percy.test.js"
    },
    "keywords": [
        "Puppeteer",
        "Jest",
        "Automation"
    ],
    "author": "Viet Hoang",
    "license": "ISC",
    "dependencies": {
        "@percy/cli": "^1.0.0-beta.42",
        "@percy/puppeteer": "^2.0.0",
        "hosted-git-info": "^4.0.2",
        "jest": "^26.6.3",
        "jest-cli": "^26.6.3",
        "jest-image-snapshot": "^4.4.0",
        "prettier": "^2.2.1",
        "puppeteer": "^9.1.1"
    }
}
